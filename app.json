{
  "expo": {
    "name": "MoodSounds",
    "slug": "mood-sounds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": ".src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": ".src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6200EE"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ".src/assets/adaptive-icon.png",
        "backgroundColor": "#6200EE"
      }
    },
    "web": {
      "favicon": ".src/assets/favicon.png"
    }
  }
}